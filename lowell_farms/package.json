    "apollo-boost": "^0.1.16",
    "apollo-client": "^2.4.2",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-styled-components": "^1.7.1",
    "base-64": "^0.1.0",
    "cookie": "^0.3.1",
    "date-fns": "^2.0.0-alpha.7",
    "downshift": "^2.2.3",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
