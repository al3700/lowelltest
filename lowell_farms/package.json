    "global": "^4.3.2",
    "google-distance-matrix": "^1.1.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "i": "^0.3.6",
    "js-cookie": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "next": "^11.1.3",
    "next-routes": "^1.4.2",
    "next-with-apollo": "^3.4.0",
    "npm": "^6.14.2",
    "nprogress": "^0.2.0",
    "prismic-javascript": "^2.0.0-beta.0",
    "prismic-reactjs": "^0.3.2",
    "prop-types": "^15.6.2",
    "react": "^16.10.2",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.2.1",
    "react-dom": "^16.10.2",
    "react-ga": "^2.5.7",
